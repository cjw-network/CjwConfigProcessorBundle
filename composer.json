{
  "name": "cjw-network/cjw-config-processor",
  "type": "ezplatform-bundle",
  "description": "Parses the configuration of your symfony server and allows visual access to the configuration in a human readable way for a better development experience.",
  "keywords": [
    "ezplatform",
    "cjw-network",
    "cjw-config-processor",
    "siteaccess",
    "config-parser",
    "yaml-parser"
  ],
  "license": "GPL-2.0-only",
  "homepage": "https://github.com/cjw-network/CjwConfigProcessorBundle",
  "authors": [
    {
      "name": "CJW-Network",
      "homepage": "https://www.cjw-network.com/"
    },
    {
      "name": "Frederic Bauer"
    }
  ],
  "require": {
    "php": "^7.3",
    "ezsystems/ezplatform-kernel": "^1.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "CJW\\CJWConfigProcessor\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
